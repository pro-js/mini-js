<!DOCTYPE html>
<html lang="en">
<head>
  <link rel='shortcut icon' type='image/x-icon' href='./img/favicon.ico' />
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>File Management</title>

  <!--- CSS -->
  <link rel="stylesheet" href="./css/materialize.min.css">
  <link rel="stylesheet" href="./css/icon.css">
  <link rel="stylesheet" href="./css/style.css">
</head>
<body>
  
  <div class="customContainer">
    <div class="row">
      <form action="/api/photo/upload" enctype="multipart/form-data" method="post">
        <div class="col s12 m6">
          <div class="card center-align hoverable waves-light lighten-1">
            <div class="card-content black-text">
              <span class="card-title">
                <h5 class="center">
                  <i class="material-icons">cloud_upload</i> 
                  Upload Session
                </h5>
              </span>
              <br>
              <input type="file" name="upload" multiple="multiple">
              <input class="btn" type="submit" value="Upload" id="uploadBtn">
            </div>
          </div>
        </div>
      </form>
    </div>
    <div class="row">
      <div id="photosID"></div>
    </div>
  </div>

  <!--- JS -->
  <script src="./js/jquery.min.js"></script>
  <script src="./js/materialize.min.js"></script>
  <script src="./js/clientControl.js"></script>
</body>
</html>